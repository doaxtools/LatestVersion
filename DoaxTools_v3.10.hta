//核心函数，四个参数分别是“对RADIO的描述”，“表上方文字”，“表的内容数组”，“备注内容”。
function addOtherData(radioText,TEXT,arrName,remark) {
    var SN = document.querySelector('#otherDataP').querySelectorAll('font').length ;
    var basicName = 'otherData' + SN ;
    createOtherDataInput(basicName,radioText);
    createOtherDataDiv(basicName);
    if (TEXT) {createOtherDateText(basicName,TEXT);}
    if (arrName) {createOtherDataTab(basicName,arrName);}
    if (remark) {createOtherDateRemark(basicName,remark);}
    changeFontColor();
}

//在otherDataP标签中创建一个radio和一个font标签
function createOtherDataInput(basicName,radioText) {
    var input = document.createElement('input');
    input.type = "radio";
    input.name = "otherDataRadio";
    input.value = basicName;
    input.id = (basicName + 'Radio');
    input.onclick = function(){onclickRadio(basicName + 'Radio') }
    otherDataP.appendChild(input);
    var font = document.createElement('font');
    font.innerHTML = radioText + '&nbsp;';
    font.id = basicName ;
    if (basicName == 'otherData1') {document.getElementById(basicName + 'Radio').checked = 'true'}
    otherDataP.appendChild(font);
}

//在otherDataDiv中创建一个新的div
function createOtherDataDiv(basicName) {
    var div = document.createElement('div');
    div.id = basicName + 'Div';
    if (basicName == 'otherData1') {
        div.style.cssText = "display:block; text-align:center;"
    } else {
        div.style.cssText = "display:none; text-align:center;";
    }
    otherDataDiv.appendChild(div);
}

//在新建的div中创建一个表
function createOtherDataTab(basicName,arrName) {
    var div = document.getElementById(basicName + 'Div') ;
    var tab = document.createElement('table');
    tab.className = 'altrowstable' ; 
    tab.id = basicName + 'Table' ;
    div.appendChild(tab);
    for (var i = 0; i < arrName.length; i++) {
        //创建行
        trs = document.createElement('tr');
        var tbody = document.querySelector('#' + basicName + 'Table');
        tbody.appendChild(trs);
        for(var k in arrName[i]) {
            //创建单元格
            var td=document.createElement('td');
            //把对象里面的属性值 给td
            td.innerHTML=arrName[i][k];
            trs.appendChild(td);
        }
    }
}

//在新建的div中创建一个p标签用来录入文字（文字位于表的上方）
function createOtherDateText(basicName,TEXT) {
    var div = document.getElementById(basicName + 'Div') ;
    var p = document.createElement('p');
    p.id = basicName + 'TextP';
    p.style.cssText = "text-align:left;";
    if (TEXT) {p.innerHTML = TEXT;}
    div.appendChild(p);
}

//在新建的div中创建一个p标签用来录入备注（备注位于表的下方）
function createOtherDateRemark(basicName,remark) {
    var div = document.getElementById(basicName + 'Div') ;
    var p = document.createElement('p');
    p.id = basicName + 'RemarkP';
    p.style.cssText = "text-align:left; color:#696969;";
    if (remark) {p.innerHTML = remark;}
    div.appendChild(p);
}

//逐个更改radio描述性文字的颜色
function changeFontColor() {
    var colorArr = [ '#000000', '#c70000', '#fc870d', '#824200', '#ff00ff', '#0000ff', '#008080', '#2F4F4F', '#191970', '#6aa84f' ]
    radios = document.querySelector('#otherDataP').querySelectorAll('font')
    for(var i = 0; i < radios.length; i++ ) {
        radios[i].color = colorArr[i];
    }
}

//重新定义文字的大小，三种用法。例：resizeFont(15,'table')仅将表格内文字大小指定为15px; resizeFont(15,'text')仅将表格之外的文字大小指定为15px; resizeFont(15)将所有文字都指定为15px.
function resizeFont(size,obj) {
    var SN = document.querySelector('#otherDataP').querySelectorAll('input').length;
    if (document.getElementById('otherData' + SN + 'Table') && obj != "text") {document.getElementById('otherData' + SN + 'Table').style.fontSize = size + 'px'}
    if (document.getElementById('otherData' + SN + 'Div') && obj != "table") {document.getElementById('otherData' + SN + 'Div').style.fontSize = size + 'px'}
}

//设置粗体字，用法：tableFontWeight()将首行设置成粗体; tableFontWeight(i)将整个第i行设置成粗体; tableFontWeight('',j)将整个第j列设置成粗体; tableFontWeight(i,j)将第i行，第j列的单个表格设置成粗体
function tableFontWeight(i,j) {
    if(i === 0) {i = '0'}
    if(j === 0) {j = '0'}
    if(!i) {i = ''}
    if(!j) {j = ''}
    var SN = document.querySelector('#otherDataP').querySelectorAll('input').length;
    var tb = document.getElementById('otherData' + SN + 'Table');
    if (i == ''){
        if (j == '') {tb.rows[0].style.fontWeight="bolder";} else {
            for (var k = 0; k < tb.rows.length; k++) {
               if (tb.rows[k].cells[j]) { tb.rows[k].cells[j].style.fontWeight="bolder"; }
            }
        }
    } else{
        if (j == '') {tb.rows[i].style.fontWeight="bolder";} else {
            if (tb.rows[i].cells[j]) { tb.rows[i].cells[j].style.fontWeight="bolder"; }
        }
    }
}

//将i行j列向右扩展k个单元格
function tableColSpan(i,j,k) {
    var SN = document.querySelector('#otherDataP').querySelectorAll('input').length;
    var tb = document.getElementById('otherData' + SN + 'Table');
    tb.rows[i].cells[j].colSpan = (tb.rows[i].cells[j].colSpan) + parseInt(k)
}

//将i行j列向下扩展k个单元格
function tableRowSpan(i,j,k) {
    var SN = document.querySelector('#otherDataP').querySelectorAll('input').length;
    var tb = document.getElementById('otherData' + SN + 'Table');
    tb.rows[i].cells[j].rowSpan = (tb.rows[i].cells[j].rowSpan) + parseInt(k)
}


</script>




