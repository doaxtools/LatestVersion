//设置键盘只允许输入整数，且最大值为maxValue，最小值minValue，该函数应用于几乎所有的输入框。
var lastValue="", lastID=""
function onkeyuper(inputID,minValue,maxValue) {
    if (lastID!=inputID) {lastValue=""};
    if (inputID.value.search("^[0-9]*$")==-1) {
        if (inputID.value!="") {
            inputID.value=lastValue
        }
        else {
            lastValue=""
        }
    }
    else {
        if (parseInt(inputID.value)<parseInt(minValue) || parseInt(inputID.value)>parseInt(maxValue) ) {
        	  inputID.value=lastValue
        }
        else {
            lastValue=inputID.value
        }
    }
    lastID = inputID
}

//根据岛主等级自动计算出相应的FP值并写入右侧输入框，该函数同时应用于女孩升级和代币计算两个部分
function writeFP(Level,fp) {
    //var Level=matchForm.yourLevel.value
    if(eval(0)<parseInt(Level)&&parseInt(Level)<=eval(30)) {
        document.getElementById(fp).value=204-(30-Level)*6
    }
    else if(eval(30)<parseInt(Level)&&parseInt(Level)<=eval(100)) {
        document.getElementById(fp).value=414-(100-Level)*3
    }
    else if(parseInt(Level)>eval(100)) {
        document.getElementById(fp).value=(Level-100)*2+414
    }
    else {
        document.getElementById(fp).value=""
    }
}

//计算当前等级升级到下级的经验值(假设当前的经验值为0)
function witeNextExp() {
    if(matchForm.currentGirlLevel.value=="") { document.getElementById("nextExp").value="" }
    else { document.getElementById("nextExp").value=levelValue[matchForm.currentGirlLevel.value] }
}

//计算升级至目标等级所需要的总经验
function writeTotalExp() {
    if (matchForm.currentGirlLevel.value!="" && matchForm.targetGirlLevel.value!="" && parseInt(matchForm.targetGirlLevel.value)>parseInt(matchForm.currentGirlLevel.value) && matchForm.nextExp.value!="") {
        i = parseInt(matchForm.currentGirlLevel.value)
        var totalvalue=0
        do {
            totalvalue+=levelValue[i]
            i++;
        }
        while(parseInt(i)<parseInt(matchForm.targetGirlLevel.value));
        document.getElementById("totalExp").value=totalvalue+parseInt(matchForm.nextExp.value)-parseInt(levelValue[matchForm.currentGirlLevel.value])
    }
    else {
        document.getElementById("totalExp").value=""
    }
}

//计算需要比赛的总局数
function writeTotalNum() {
    if(matchForm.totalExp.value=="" || matchForm.exp.value=="") { document.getElementById("totalMatchNum").value="" }
    else { document.getElementById("totalMatchNum").value=Math.ceil(parseInt(matchForm.totalExp.value)/parseInt(matchForm.exp.value)) }
}

//计算耗水量
function writeWaterNum() {
    if(matchForm.totalMatchNum.value!="" && matchForm.neededFP.value!="" && matchForm.fp.value!=""){ document.getElementById("totalWaterNum").value=Math.ceil(parseInt(matchForm.totalMatchNum.value)*parseInt(matchForm.neededFP.value)/parseInt(matchForm.fp.value)) }
    else{ document.getElementById("totalWaterNum").value="" }
}



