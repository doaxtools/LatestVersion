//使表格奇偶行颜色错落
function altRows(id,evenrow,oddrow){
    if(document.getElementsByTagName){
        var table = document.getElementById(id); 
        var rows = table.getElementsByTagName("tr");          
        for(i = 0; i < rows.length; i++){         
            if(i % 2 == 0){
                rows[i].className = evenrow;
            }else{
                rows[i].className = oddrow;
            }     
        }
    }
}

function altRowsClass(ClassName,evenrow,oddrow){
    if(document.getElementsByTagName){
        if(!evenrow) { var evenrow = 'evenrowcolor' }
        if(!oddrow) { var oddrow = 'oddrowcolor' }
        for (j = 0; j < document.getElementsByClassName(ClassName).length; j++) {
            var id = document.getElementsByClassName(ClassName)[j].id
            var table = document.getElementById(id);
            var rows = table.getElementsByTagName("tr");       
            for(i = 0; i < rows.length; i++){
                if(i % 2 == 0){
                    rows[i].className = evenrow;
                }else{
                    rows[i].className = oddrow;
                }
            }
        }
    }
}
