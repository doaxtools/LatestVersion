//将girlAttArr数组的有用信息分别生成力量女孩、技巧女孩、体力女孩三个表格
function createGirlAttTab(att) {
    for (var i = 0; i < girlAttArr.length; i++) {
        //创建行
        if(girlAttArr[i][9] == att) {
            trs = document.createElement('tr');
            var tbody = document.querySelector(('#'+att));
            tbody.appendChild(trs);
            var arr=[1,11,2,3,4,5,6,7,8,10]
            girlAttArr[i][11]=1
            for(var k in arr) {
                //创建单元格
                var td=document.createElement('td');
                //把对象里面的属性值 给td
                td.innerHTML=girlAttArr[i][arr[k]];
                trs.appendChild(td);
            }
        }
    }
}




