//负责关键函数的调用，根据input的查询日期调用函数
function getHistoryCampaign(date){
    lastDay=historyCampaignSourceArr[0][0]
    document.getElementById('waitingSign1').style.color='red'
    document.getElementById('waitingSign2').style.color='red'
    document.getElementById('waitingSign3').style.color='red'
    if (date >= '191210' && date < lastDay) {
        document.getElementById('waitingSign1').style.color='green'
        for(i=0; i<historyCampaignSourceArr.length; i++) { 
            if(historyCampaignSourceArr[i][0] <= date) {
                eventDate = historyCampaignSourceArr[i][0];
                campaignDesc = historyCampaignSourceArr[i][1];
                hisUrl = historyCampaignSourceArr[i][2];
                break;
            }
        }
        matchExistingSource(hisUrl);
    } else if (date >= '190327' && date < '191210') {
        document.getElementById('waitingSign1').style.color='green'
        for(i=0; i<oldCampaignSourceArr.length; i++){
            if(oldCampaignSourceArr[i][0] <= date){
                eventDate = oldCampaignSourceArr[i][0];
                campaignDesc = oldCampaignSourceArr[i][1];
                oldUrl = oldCampaignSourceArr[i][2];
                break;
            }
        }
        matchExistingSource(oldUrl);
    } else if (date < '190327') {
        document.getElementById('waitingSign1').style.color='green'
        alert("游戏在2019年03月27日开始发售！");
        document.getElementById("source").value = 'https://game.doaxvv.com/cn/info/361.html'
        document.getElementById('imgHref').href = 'https://game.doaxvv.com/cn/info/361.html'
        document.getElementById('campaignDisc').innerHTML = '&nbsp;190327:<a title="点击前往活动页面" style="text-decoration:none;color:blue" href="#" rel="external nofollow" onclick="useDefBrowser(\'https://game.doaxvv.com/cn/info/361.html\')">“第一次的睡衣派对”的通知</a>'; 
        document.getElementById('waitingSign2').style.color='green'
        document.getElementById('waitingSign0').innerHTML=''
        imgLoad('https://game.doaxvv.com/wp-content/uploads/2019/03/1a1_gacha_m_cn.png',imgLoaded); 
    } else {
        getPageSource(date);
    }
}

