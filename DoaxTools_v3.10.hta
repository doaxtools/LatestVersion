function getPageSource(inputDate,startPage,endPage) {
    if(!startPage || startPage<1){startPage=1;}
    if(!endPage || endPage==0){endPage=200;}
    i = startPage; //alert(i);
    j = endPage;
    d = inputDate;
    i == 1? pageUrl = 'https://game.doaxvv.com/cn/news_list/index.html' : pageUrl = 'https://game.doaxvv.com/cn/news_list/page/' + i + '/index.html' ;
    xmlHttp = new XMLHttpRequest();                  //创建XMLHttpRequest对象
    xmlHttp.onreadystatechange = getCampaignSource;       //设置回调函数
    xmlHttp.open("GET", pageUrl, true);
    xmlHttp.send(null);
}

//获取每一页的活动链接，匹配到相应结束日期(inputDate)或结束页码(EndPage)则函数循环结束。匹配inputDate时将结果传递给getImgSource继续执行后续命令，匹配EndPage时将得到的活动URL数组写入textarea.
function getCampaignSource() {
    if (xmlHttp.readyState == 4) {
        var pageCampaignUrlArr = xmlHttp.responseText.match(/http(s)?\:\/\/game.doaxvv.com\/production\/html\/information\/event_gl_(.*?)_cn.html(\?GameView=Y)?/g);
        var pageCampaignFieldArr = xmlHttp.responseText.match(/http(s)?\:\/\/game.doaxvv.com\/production\/html\/information\/event_gl_(.*?)<\/p><\/a><\/li>/g);
        i++; 
        if(pageCampaignFieldArr && pageCampaignUrlArr) {
            pageCampaignFieldNum = pageCampaignFieldArr.length
            campaignFieldNum = campaignFieldArr.length
            campaignUrlNum = campaignUrlArr.length
            lastEventDate = pageCampaignFieldArr[pageCampaignFieldNum-1].match(/[0-9]{2}\.[0-9]{2}\.[0-9]{2}/)[0].replace(/\./g,''); //alert(lastEventDate);
            //lastEventDate = pageCampaignUrlArr[pageCampaignUrlNum-1].match(/_[0-9]{6,8}_/)[0].match(/[0-9]{6}_/)[0].match(/[0-9]{6}/)[0];
            if(lastEventDate > d && i <= j) {  
                if(campaignUrlArr != '') {
                    campaignUrlArr = campaignUrlArr.concat(pageCampaignUrlArr); 
                } else {
                    campaignUrlArr = pageCampaignUrlArr; 
                } ; 
                //alert(campaignUrlArr);
                if(document.getElementById("sourceBox").checked) { 
                    document.getElementById("source").value = campaignUrlArr 
                    if ( lastEventDate <= 191210 ) {
                        document.getElementById('waitingSign0').innerHTML='&#x270C;';
                        document.getElementById('waitingSign0').style.color='green';
                        return;
                    }
                }
                getPageSource(d,i,j)
            } else { 
                for(k=0; k<pageCampaignFieldNum; k++) { 
                    eventDate = pageCampaignFieldArr[k].match(/[0-9]{2}\.[0-9]{2}\.[0-9]{2}/)[0].replace(/\./g,'');
                    campaignUrlArr[campaignUrlNum+k] = pageCampaignUrlArr[k] ;
                    campaignDesc = pageCampaignFieldArr[k].match(/<p>(.*?)<\/p>/)[0].replace('<p>','').replace('</p>','');
                    if(eventDate <= d) { break; }
                }
                if(document.getElementById("sourceBox").checked) { document.getElementById("source").value = campaignUrlArr }
                document.getElementById('waitingSign1').style.color='green'; 
                document.getElementById('waitingSign0').innerHTML='&#x270C;'
                document.getElementById('waitingSign0').style.color='green'; 
                if (i>j) { return; }
                campaignUrl = pageCampaignUrlArr[k]; 
                //alert(eventDate + ': ' + campaignDesc);
                xmlHttp = new XMLHttpRequest();
                xmlHttp.onreadystatechange = getImgSource;
                xmlHttp.open("GET", campaignUrl, true);
                xmlHttp.send(null);
            }
        } else {
            if (i>=j) { 
                if(document.getElementById("sourceBox").checked) { 
                    document.getElementById("source").value = campaignUrlArr ;
                    document.getElementById('waitingSign0').innerHTML='&#x270C;'
                    document.getElementById('waitingSign0').style.color='green'; 
                }
                return; 
            }
            getPageSource(d,i,j)
        }
    }
}

