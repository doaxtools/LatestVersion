        var partFieldArr = xmlHttp.responseText.match(/http(s)?\:\/\/game.doaxvv.com\/production\/html\/information\/event_gl_(.*?)<\/p><\/a><\/li>/g);
        //var partUrlArr = xmlHttp.responseText.match(/http(s)?\:\/\/game.doaxvv.com\/production\/html\/information\/event_gl_(.*?)_cn.html(\?GameView=Y)?/g);
        i++;
        if(partFieldArr) {
            for(l = 0; l < partFieldArr.length; l++) {
                newPartFieldArr[l] = [partFieldArr[l].match(/[0-9]{2}\.[0-9]{2}\.[0-9]{2}/)[0].replace(/\./g,''),partFieldArr[l].match(/<p>(.*?)<\/p>/)[0].replace('<p>','').replace('</p>',''),partFieldArr[l].match(/http(s)?\:\/\/game.doaxvv.com\/production\/html\/information\/event_gl_(.*?)_cn.html(\?GameView=Y)?/)[0]];
                if(newPartFieldArr[l][0] <= historyCampaignSourceArr[0][0]) { break; } else {
                    campaignUrlArr = campaignUrlArr + "[\"" + newPartFieldArr[l][0] + "\",\"" + newPartFieldArr[l][1] + "\",\"" + newPartFieldArr[l][2] + "\"]," ;
