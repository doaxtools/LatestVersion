            //alert(partFieldArr[0].match(/[0-9]{2}\.[0-9]{2}\.[0-9]{2}/)[0].replace(/\./g,''));
            if(partFieldArr[partFieldArr.length-1].match(/[0-9]{2}\.[0-9]{2}\.[0-9]{2}/)[0].replace(/\./g,'') <= historyCampaignSourceArr[0][0]) {
