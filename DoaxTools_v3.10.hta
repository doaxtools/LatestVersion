//被其他函数调用，根据其调用函数传递的URL地址，抓取匹配的图片，并将图片反馈至页面同时将传递来的URL值写入textarea并反馈结束信号
function getImgSource(){
    if (xmlHttp.readyState == 4) {
        var imgUrl = xmlHttp.responseText.match(/img\/(.*?)_cn.png/); 
        if(imgUrl) {
            imgUrl = imgUrl[0]
            imgUrl = 'https://game.doaxvv.com/production/html/information/' + imgUrl
        } else {
            imgUrl = xmlHttp.responseText.match(/img(.*?)_cn.png/); 
            if(imgUrl) {
                imgUrl = imgUrl[0].match(/\/(.*?)_cn.png/)[0]; //alert(imgUrl);
                imgUrl = 'https://game.doaxvv.com/' + imgUrl
            } else { imgUrl = '' }
        }
        if(!document.getElementById("sourceBox").checked) {
            document.getElementById('source').value = campaignUrl ; 
            document.getElementById('waitingSign0').innerHTML=''
            imgLoad(imgUrl,imgLoaded); 
        }
        document.getElementById('imgHref').href = campaignUrl; 
        document.getElementById('campaignDisc').innerHTML = '&nbsp;' + eventDate + ':' + '<a title="点击前往活动页面" style="text-decoration:none;color:blue" href="#" rel="external nofollow" onclick="useDefBrowser(campaignUrl)">' + campaignDesc + '</a>'; 
        document.getElementById('waitingSign2').style.color='green'
    }
}

