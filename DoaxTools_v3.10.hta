//计算相应等级提升了多少属性值，返回值为[力量提升,技巧提升,体力提升]的一个数组
function proAdds(level) {
    var addtec = 0 , addpow = 0 , addstm = 0
    var proAddArr = new Array() ;
    var proAddArrs = new Array() ;
    proAddArrs[0]=[0,0,0];
    for(var i=0; i<proAddArr50.length+50 ; i++) {
        if(i<50) {
            proAddArr[i] = [12,12,16]
        } else {
            proAddArr[i] = proAddArr50[i-50]
        }
        proAddArr[0] = [0,0,0];
        addpow+=proAddArr[i][0];
        addtec+=proAddArr[i][1];
        addstm+=proAddArr[i][2];
        proAddArrs[i+1] = [addpow,addtec,addstm]
    }
    return proAddArrs[level]; 
}

