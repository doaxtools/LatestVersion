var giftArr=[['&hearts;',5,0,0],['&hearts;',20,0,0],['&hearts;&hearts;',250,0,0],['&hearts;&hearts;&hearts;',500,0,0],['爱的奖励',20000,0,0],['感恩的表示',10000,0,0],['永恒闪耀天然石',5000,0,0],['幸福四叶草',1000,0,0],['其他礼物①',0,0,0],['其他礼物②',0,0,0],['统计',0]]
