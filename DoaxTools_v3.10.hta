//为几率计算的"卡池人数"项动态生成下拉菜单.
function addPRcalSelect2() {
    var obj = document.getElementById('PRcalSelect2');
    if (document.getElementById('PRcalSelect1').value == "poolSelect2" ) {
        obj.options.length = 0;
        obj.options.add(new Option("2人池","2"));
    } else if (document.getElementById('PRcalSelect1').value == "poolSelect6") {
        obj.options.length = 0;
        obj.options.add(new Option("单人池","1"));
    } else {
        obj.options.length = 0;
        for(var i=3; i<=girlAttArr.length; i++) {
            obj.options.add(new Option(i + "人池",i));
        }
    }
    obj.options[0].selected = true;
}

//一般池(不包括3步3.3)的概率计算，并将结果写入页面.
function normalRate(rate) {
    var succRate=0 , succRate2=0 ;
    var girlNum = parseInt(document.getElementById('PRcalSelect2').value) ;
    var tenfoldNum = document.getElementById('tenfoldNum').value ;
    if (tenfoldNum) {
        succRate = 1-Math.pow((Math.pow((1-rate),10)), tenfoldNum);
        document.getElementById('SuccessRate1').value = succRate ;
        if (girlNum) {
            succRate2 = 1-Math.pow((Math.pow((1-rate/girlNum),10)), tenfoldNum);
            document.getElementById('SuccessRate2').value = succRate2 ;
        } else {document.getElementById('SuccessRate2').value = "" ;}
    }else{
        document.getElementById('SuccessRate1').value = "" ;
    }
}

//计算SR出货率(即战术出货率)
function srRate() {
    var srNum = document.getElementById('srNum').value ;
    var tenfoldNum = document.getElementById('tenfoldNum').value ;
    if (tenfoldNum && srNum) {
        srSuccRate = 1-Math.pow((Math.pow((1-0.1/srNum),9))*(1-0.989/srNum), tenfoldNum);
        document.getElementById('SuccessRate3').value = srSuccRate ;
    } else {document.getElementById('SuccessRate3').value = ""};
}

//3步3.3池的出货率计算
function poolSelectRate3() {
    var girlNum = parseInt(document.getElementById('PRcalSelect2').value) ;
    var tenfoldNum = document.getElementById('tenfoldNum').value ;
    var roundNum1=parseInt(tenfoldNum/3) ;
    var roundNum2=parseInt(tenfoldNum%3) ;
    if (tenfoldNum) {
        oneRoundFailRate = Math.pow(Math.pow((1-0.011),10),2)*Math.pow((1-0.033),10) ;
        succRate = 1-Math.pow(oneRoundFailRate,roundNum1)*Math.pow(Math.pow((1-0.011),10),roundNum2) ;
        document.getElementById('SuccessRate1').value = succRate ;
        if (girlNum) {
            oneRoundFailRate2 = Math.pow((Math.pow((1-0.011/girlNum),10)),2)*Math.pow((1-0.033/girlNum),10) ;
            succRate2 = 1-Math.pow(oneRoundFailRate2,roundNum1)*Math.pow(Math.pow((1-0.011/girlNum),10),roundNum2) ;
            document.getElementById('SuccessRate2').value = succRate2 ;
        } else {document.getElementById('SuccessRate2').value = "" ;}
    }else{
        document.getElementById('SuccessRate1').value = "" ;
    }
}

//几率计算的核心函数，输入框每变动一次函数执行一次
function PRcalculation() {
    var succRate=0 , succRate2=0 ;
    var PRcalSelect1 = document.getElementById('PRcalSelect1').value ;
    var stoneNum = parseInt(document.getElementById('stoneNum').value) ;
    var tenfoldNum = parseInt(stoneNum/5000) ;
    roundNum1 = parseInt(tenfoldNum/3) ;
    roundNum2 = tenfoldNum%3 ;
    if (PRcalSelect1 == "poolSelect4") { tenfoldNum = parseInt(tenfoldNum/3) + tenfoldNum } ;
    document.getElementById('tenfoldNum').value = tenfoldNum ;
    document.querySelector('#round1').style.display="none";
    document.querySelector('#round2').style.display="none";
    document.querySelector('#br').style.display="block";
    document.querySelector('#br2').style.display="none";
    document.querySelector('#sr2').style.display="none";
    document.querySelector('#ssr2').style.display="block";
    switch (PRcalSelect1) {
        case "poolSelect1" :
            document.querySelector('#sr1').style.display="none";
            normalRate(0.011);
            break;
        case "poolSelect2" :
            document.querySelector('#sr1').style.display="none";
            normalRate(0.0078);
            break;
        case "poolSelect3" :
            document.querySelector('#sr1').style.display="none";
            document.querySelector('#round1').style.display="block";
            document.querySelector('#round2').style.display="block";
            document.querySelector('#br').style.display="none";
            document.getElementById('roundNum1').value = roundNum1 ;
            document.getElementById('roundNum2').value = roundNum2 ;
            poolSelectRate3();
            break;
        case "poolSelect4" :
            document.querySelector('#sr1').style.display="none";
            document.querySelector('#round1').style.display="block";
            document.querySelector('#round2').style.display="block";
            document.querySelector('#br').style.display="none";
            document.getElementById('roundNum1').value = roundNum1 ;
            document.getElementById('roundNum2').value = roundNum2 ;
            normalRate(0.011);
            break;
        case "poolSelect5" :
            document.querySelector('#sr1').style.display="none";
            normalRate(0.033);
            break;
        case "poolSelect6" :
            document.querySelector('#sr1').style.display="block";
            document.querySelector('#sr2').style.display="block";
            document.querySelector('#ssr2').style.display="none";
            document.querySelector('#br').style.display="none";
            document.querySelector('#br2').style.display="block";
            normalRate(0.0078);
            srRate();
            break;
        default:
            break;
    }
    if (!stoneNum) {
        document.getElementById('roundNum1').value = "" ;
        document.getElementById('roundNum2').value = "" ;
        document.getElementById('tenfoldNum').value = "" ;
        document.getElementById('SuccessRate1').value = "" ;
        document.getElementById('SuccessRate2').value = "" ;
    }
}

//重置几率计算模块的数据和格式
function PRcalReset() { 
    document.getElementById("PRcalForm").reset(); 
    addPRcalSelect2();
    PRcalculation();
}




