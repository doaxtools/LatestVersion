//计算出需要比赛的场数并将结果写入matchNum.
function writeMatchNum() {
    currentScore=document.getElementById('currentScore').value ;
    targetScore=document.getElementById('targetScore').value ;
    averageScore=document.getElementById('averageScore').value ;
    if(currentScore && targetScore && averageScore && parseInt(targetScore)>parseInt(currentScore)) {
        document.getElementById('matchNum').value = Math.ceil((targetScore-currentScore)/averageScore) ;
    } else {
        document.getElementById('matchNum').value = "" ;
    }
}

//计算消耗的水量
function writeNeededWater() {
    matchNum=document.getElementById('matchNum').value;
    fp2=document.getElementById('fp2').value;
    averageFP=document.getElementById('averageFP').value;
    if (matchNum && fp2 && averageFP) {
        document.getElementById('neededWater').value = Math.ceil(matchNum*averageFP/fp2) ;
    } else {
        document.getElementById('neededWater').value = "" ;
    }
}

//计算收获多代币
function writeGetCoin() {
    matchNum=document.getElementById('matchNum').value;
    averageCoin=document.getElementById('averageCoin').value;
    winningRate=document.getElementById('winningRate').value;
    if (matchNum && averageCoin && winningRate) {
        document.getElementById('getCoin').value = Math.ceil(matchNum*averageCoin*winningRate/100);
    } else {
        document.getElementById('getCoin').value = "" ;
    }
}

//计算共提升了多少经验值
function writeGetExp() {
    matchNum=document.getElementById('matchNum').value;
    averageExp=document.getElementById('averageExp').value;
    winningRate=document.getElementById('winningRate').value;
    if (matchNum && averageExp && winningRate) {
        document.getElementById('getExp').value = parseInt(matchNum*averageExp*winningRate/100);
    } else {
        document.getElementById('getExp').value = "" ;
    }
}




