//定义了按下不同的button按键页面执行的操作
function onclickButton(buttonID) {
    buttons = document.querySelector('#buttons').querySelectorAll('input');
    for (i=0; i<buttons.length ; i++){
        if(buttonID == buttons[i].id) {
            document.getElementById(buttons[i].id.replace('Button','Div')).style.display='block';
            document.getElementById(buttons[i].id).className = 'buttonChosenClass';         
        }else{
            document.getElementById(buttons[i].id.replace('Button','Div')).style.display='none';
            document.getElementById(buttons[i].id).className = 'buttonClass';
        }
    }
}

//定义了按下不同的radio后页面执行的操作
function onclickRadio(radioID) {
    radioParentForm = document.getElementById(radioID).parentNode.parentNode;
    radios = document.querySelector('#' + radioParentForm.id).querySelectorAll('input')
    for (i=0; i<radioParentForm.length ; i++){
        if(radioID == radios[i].id) {
            document.getElementById(radios[i].id.replace('Radio','Div')).style.display='block';
            //document.getElementById(radios[i].id).className = 'radioClass';
            document.querySelector('#' + radios[i].id).checked = true;    
        }else{
            document.getElementById(radios[i].id.replace('Radio','Div')).style.display='none';
            //document.getElementById(radios[i].id).className = 'radioClass2';
        }
    }
}

